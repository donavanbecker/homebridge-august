{
  "displayName": "August",
  "name": "homebridge-august",
  "version": "1.3.4",
  "description": "The August plugin allows you to access your August & Yale device(s) from HomeKit.",
  "author": "donavanbecker",
  "license": "Apache-2.0",
  "icon": "https://raw.githubusercontent.com/donavanbecker/homebridge-august/beta-1.3.2/branding/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/donavanbecker/homebridge-august.git"
  },
  "bugs": {
    "url": "https://github.com/donavanbecker/homebridge-august/issues"
  },
  "engines": {
    "homebridge": "^1.7.0",
    "node": "^18 || ^20"
  },
  "main": "dist/index.js",
  "scripts": {
    "check": "npm install && npm outdated",
    "update": "ncu -u && npm update && npm install",
    "lint": "eslint src/**.ts",
    "watch": "npm run build && npm link && nodemon",
    "build": "rimraf ./dist && tsc",
    "prepublishOnly": "npm run update && npm run lint && npm run build",
    "postpublish": "npm run clean",
    "clean": "rimraf ./dist",
    "test": "eslint src/**.ts"
  },
  "funding": [
    {
      "type": "Paypal",
      "url": "https://paypal.me/donavanbecker"
    },
    {
      "type": "GitHub",
      "url": "https://github.com/sponsors/donavanbecker"
    }
  ],
  "keywords": [
    "homebridge-plugin",
    "august",
    "yale",
    "lock"
  ],
  "dependencies": {
    "august-api": "^1.5.1",
    "rxjs": "^7.8.1",
    "@homebridge/plugin-ui-utils": "^1.0.1",
    "super-stringify": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "homebridge": "^1.7.0",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "npm-check-updates": "^16.14.12"
  }
}
